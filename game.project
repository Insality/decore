[bootstrap]
main_collection = /test/test.collectionc

[script]
shared_state = 1

[display]
width = 960
height = 640

[android]
input_method = HiddenInputField

[project]
title = Decore
version = 1
publisher = Insality
developer = Maksim Tuprikov, Insality
custom_resources = /resources
dependencies#0 = https://github.com/britzl/deftest/archive/master.zip

[library]
include_dirs = decore

[log]
level = ERROR
level_release = WARN
time_tracking = 1
memory_tracking = 1
info_block_length = 18
info_block = %levelshort[%logger
message_block = ]: %message %context %tab<%function>

